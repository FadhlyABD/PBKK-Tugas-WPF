            <Image HorizontalAlignment="Left" Height="25" Margin="18,254,0,0" VerticalAlignment="Top" Width="25" Source="/icon/home.png" Stretch="None"/>
            <Button Content="Analysis" HorizontalAlignment="Center" Margin="0,304,0,0" VerticalAlignment="Top" Height="65" Width="224" Click="Button_Click" Background="{x:Null}" FontFamily="Nirmala UI" Foreground="#FF007EF9" FontSize="20" BorderBrush="{x:Null}" Cursor="Hand"/>
            <Image HorizontalAlignment="Left" Height="25" Margin="18,324,0,0" VerticalAlignment="Top" Width="25" Source="/icon/diagram.png" Stretch="None"/>
            <Button Content="Calendar" HorizontalAlignment="Center" Margin="0,373,0,0" VerticalAlignment="Top" Height="65" Width="224" Click="Button_Click" Background="{x:Null}" FontFamily="Nirmala UI" Foreground="#FF007EF9" FontSize="20" BorderBrush="{x:Null}" Cursor="Hand"/>
