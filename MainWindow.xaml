        </Grid>
        <Button Content="Settings" HorizontalAlignment="Left" Margin="971,9,0,0" VerticalAlignment="Top" Height="28" Width="144" Click="Button_Click" Background="{x:Null}" FontFamily="Nirmala UI" Foreground="#FF007EF9" FontSize="16" BorderBrush="{x:Null}" Cursor="Hand"/>
        <Image HorizontalAlignment="Left" Height="22" Margin="986,12,0,0" VerticalAlignment="Top" Width="24" Source="/icon/settings.png" Stretch="None"/>
        <Label Content="Dashboard" HorizontalAlignment="Left" Margin="284,87,0,0" VerticalAlignment="Top" Foreground="#FF9EA1B0" FontSize="36"/>
        <TextBlock HorizontalAlignment="Left" Margin="828,105,0,0" TextWrapping="Wrap" Text="Search for something...." VerticalAlignment="Top" Foreground="#FF9EA1B0" Background="#FF4A4F63" Padding="12,2,0,2" Width="315" FontSize="14"/>
        <Grid Margin="284,182,600,325" Background="#FF252A40">
            <Label Content="Earnings" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Foreground="#FFC9C9C9" FontSize="20"/>
            <Label Content="Rp900.000,00" HorizontalAlignment="Left" Margin="10,0,0,0" VerticalAlignment="Center" Foreground="#FF007EF9" FontSize="26" FontFamily="Microsoft Sans Serif" FontWeight="Bold"/>
