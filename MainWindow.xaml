            <Image HorizontalAlignment="Center" Height="100" Margin="0,20,0,0" VerticalAlignment="Top" Width="100" Source="/icon/Untitled-11.png"/>
            <Label Content="FadhlyABD" HorizontalAlignment="Center" Margin="0,125,0,0" VerticalAlignment="Top" Foreground="#FF007EF9" FontSize="20" FontFamily="Nirmala UI"/>
            <Label Content="Welcome to my WPF Dashboard!" HorizontalAlignment="Center" Margin="0,167,0,0" VerticalAlignment="Top" Foreground="#FF9EA1B0" FontSize="14"/>
        </Grid>
        <Grid Margin="224,0,0,601" Background="#FF181E36" MouseLeftButtonDown="Grid_MouseLeftButtonDown">
