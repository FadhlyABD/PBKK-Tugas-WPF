            <Label Content="15M+" HorizontalAlignment="Left" Margin="15,0,0,0" VerticalAlignment="Center" Foreground="#FF32E2B2" FontSize="26" FontFamily="Microsoft Sans Serif" FontWeight="Bold"/>
            <Label Content="Details of lasts 30 days" HorizontalAlignment="Left" Margin="15,96,0,0" VerticalAlignment="Top" Foreground="#FF9EA1B0" FontSize="16"/>
        </Grid>
        <Grid Margin="946,182,30,325" Background="#FF252A40">
            <Label Content="Notes" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Foreground="#FFC9C9C9" FontSize="20"/>
            <Label Content="Some extra text here &#xD;&#xA;to make the tab &#xD;&#xA;more attractive" HorizontalAlignment="Left" Margin="10,52,0,0" VerticalAlignment="Top" Foreground="#FF9EA1B0" FontSize="16"/>
        </Grid>
        <Grid Margin="615,339,30,84" Background="#FF252A40">
            <Label Content="Some Graphs" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Foreground="#FFC9C9C9" FontSize="20"/>
        </Grid>
        <Grid Margin="615,579,30,10" Background="#FF252A40">
            <Label Content="Some Texts" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Foreground="#FFC9C9C9" FontSize="20"/>
        </Grid>
        <Image HorizontalAlignment="Left" Height="113" Margin="495,194,0,0" VerticalAlignment="Top" Width="104" Source="/icon/money-bag.png"/>
        <Image HorizontalAlignment="Left" Height="93" Margin="793,207,0,0" VerticalAlignment="Top" Width="130" Source="/icon/downloads.png"/>
        <Image HorizontalAlignment="Left" Height="180" Margin="647,388,0,0" VerticalAlignment="Top" Width="491" Source="/icon/chart-diagram.png"/>
        <ProgressBar HorizontalAlignment="Left" Height="32" Margin="356,420,0,0" VerticalAlignment="Top" Width="172" Value="67" Foreground="#FF007EF9" BorderBrush="{x:Null}" Background="#FF181E36"/>
    </Grid>
